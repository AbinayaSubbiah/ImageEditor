{
    "name": "dropdowns",
    "description": "DropDown Components",
    "keywords": "ej2-dropdownlist, ej2-combobox, ej2-autocomplete and ej2-multiselect",
    "components": [
        {
            "baseClass": "DropDownList",
            "directoryName": "drop-down-list",
            "preferredTag": "input",
            "skipEventPropagation": true,
            "type": "form",
            "twoWays": [
                "value"
            ],
            "comment": [
                "/**",
                "*The DropDownList component contains a list of predefined values, from which the user can choose a single value.",
                "*```html",
                "*<ej-dropdownlist></ej-dropdownlist>",
                "*```",
                "*/"
            ],
            "reactComment": [
                "/**",
                " * The DropDownList component contains a list of predefined values, from which the user can choose a single value.",
                " * ```",
                " * <DropDownListComponent dataSource={data}/>",
                " * ```",
                " */"
            ],
            "templateProperties": [
                "footerTemplate",
                "headerTemplate",
                "valueTemplate",
                "groupTemplate",
                "itemTemplate",
                {
                    "name": "noRecordsTemplate",
                    "defaultValue": "No Records Found"
                },
                {
                    "name": "actionFailureTemplate",
                    "defaultValue": "The Request Failed"
                }
            ]
        },
        {
